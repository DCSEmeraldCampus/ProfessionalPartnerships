@model ProfessionalPartnerships.Data.Models.ConfigurationValues
@{
    ViewData["Title"] = "Edit Configuration Value";
}

<h2>Edit Configuration Values</h2>
@using (Html.BeginForm("EditConfigurationValue", "ConfigurationValues", FormMethod.Post))
{
    @Html.HiddenFor(model => model.ConfigurationValueId)
    
    <div class="form-group">
        @Html.LabelFor(model => model.Key)
        @Html.DisplayTextFor(model => model.Key)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Value)
        @Html.TextBoxFor(model => model.Value)
    </div>

    <input type="submit" value="Edit" />
}
